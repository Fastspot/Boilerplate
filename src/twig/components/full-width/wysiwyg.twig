<!-- WYSIWYG -->
<div class="wysiwyg_block">
	<div class="wysiwyg_block_inner">
		<div class="fs-row">
			<div class="fs-cell">
				<div class="wysiwyg_block_wrapper">
					<div class="typography" style="color: red;">
						{{content}}
						{{site.icon("arrow_right")}}
						{{site.icon("clock")}}
						{{site.icon("close")}}
						{{site.icon("directional")}}
						{{site.icon("facebook")}}
						{{site.icon("heart")}}
						{{site.icon("home")}}
						{{site.icon("linkedin")}}
						{{site.icon("logo")}}
						{{site.icon("menu_search")}}
						{{site.icon("menu")}}
						{{site.icon("pencil")}}
						{{site.icon("phone")}}
						{{site.icon("place")}}
						{{site.icon("play")}}
						{{site.icon("quote")}}
						{{site.icon("search")}}
						{{site.icon("triangle_down")}}
						{{site.icon("twitter")}}
						{{site.icon("vimeo")}}
						{{site.icon("youtube")}}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- END: WYSIWYG -->
