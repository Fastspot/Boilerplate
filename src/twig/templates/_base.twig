{# Shortcuts For Use Within Components #}
{% set feat = "../components/feature/" %}
{% set full = "../components/full-width/" %}
{% set inc = "../components/in-content/" %}
{% set side = "../components/sidebar/" %}
{% set par = "../../partials/" %}

{% set grid = true %}

{% block page %}
	{% set page = {
		title: "Page",
		layout: "default"
	} %}
{% endblock %}

{% block activePage %}
	{% set activePage = 0 %}
{% endblock %}

{% block mainNav %}
	{% set mainNavAlias = links.mainNav %}
{% endblock %}

{% block secondaryNav %}
	{% set secondaryNavAlias = links.secondaryNav %}
{% endblock %}

{% block breadcrumbNav %}
	{% set breadcrumbNav = [
		page.title
	] %}
{% endblock %}

{% block sectionTitle %}
	{% set sectionTitle = page.title %}
{% endblock %}

{% block subNav %}
	{% set subNavAlias = links.subNav %}
{% endblock %}

{% block utilityNav %}
	{% set utilityNavAlias = links.utilityNav %}
{% endblock %}

{% block quickNav %}
	{% set quickNavAlias = links.quickNav %}
{% endblock %}

{% block footerNav %}
	{% set footerNavAlias = links.footerNav %}
{% endblock %}

{% block socialNav %}
	{% set socialNavAlias = links.socialNav %}
{% endblock %}

{% import '../partials/macros.twig' as site %}

{% include '../partials/base/header.twig' %}

{% block pageContent %}
{% endblock %}

{% include '../partials/base/footer.twig' %}
