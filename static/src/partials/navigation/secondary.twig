<nav class="secondary_nav {{class}}" aria-label="Secondary Navigation" itemscope itemtype="http://schema.org/SiteNavigationElement">
	<h2 class="secondary_nav_heading">Secondary Navigation</h2>
	<div class="secondary_nav_list" role="navigation">
		{% for link in links %}
			<div class="secondary_nav_item" itemprop="url">
				<a class="secondary_nav_link" href="#" itemprop="name">{{link}}</a>
			</div>
		{% endfor %}
	</div>
</nav>
